<%- if controller_name != 'sessions' %>
  <%= link_to "Je me connecte", new_session_path(resource_name), :class => "form__submit--outline" %>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Je m'inscris", new_registration_path(resource_name), :class => "form__submit--outline" %>
<% end %>
